####### 
####### Classes
#######

# Public contract
pc:Contract a owl:Class ;
  rdfs:subClassOf c4n:Call ;
  rdfs:label "Veřejná zakázka"@cs, "Public contract"@en ;
  rdfs:comment "Základní třída pro popis veřejné zakázky"@cs ;
  rdfs:comment "Basic class for decribing a public contract"@en ;
  rdfs:isDefinedBy pc: ;
  owl:equivalentClass loted:Tender
  .
  
# Tender
pc:Tender a owl:Class ;
  rdfs:subClassOf gr:Offering ;
  rdfs:label "Nabídka"@cs, "Tender"@en ;
  rdfs:comment "Základní třída pro popis nabídky dodavatele"@cs ;
  rdfs:comment "Basic class for decribing a tender submitted by a supplier"@en ;
  rdfs:isDefinedBy pc:
  .
  
# Default contract award criteria type: combination of criteria (= most economically advantageous award criteria type)
pc:AwardCriteriaCombination a owl:Class ;
  owl:equivalentClass loted:AwardCriteria ;
  rdfs:label "Kombinace kritérií pro hodnocení dodaných nabídek"@cs, "Combination of contract award criteria"@en ;
  rdfs:comment "Třída pro popis kombinace kritérií pro posuzování nabídek dodavatelů"@cs ;
  rdfs:comment "Class for description of criteria combination used for tender evaluation"@en ;
  rdfs:isDefinedBy pc:
  .
 
# Contract award criteria type: lowest price
pc:AwardCriteriaLowestPrice a owl:Class ;
  rdfs:subClassOf pc:AwardCriteriaCombination ;
  rdfs:subClassOf [
	a owl:Restriction ;
    owl:onProperty pc:awardCriterion ;
    owl:maxCardinality "0"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Nejnižší cena je jediným rozhodujícím kritériem při hodnocení dodaných nabídek"@cs, "Lowest price is the only contract award criterion"@en ;
  rdfs:comment "Třída specifikující že jediným kritériem pro posuzování nabídek dodavatelů je nejnižší cena"@cs ;
  rdfs:comment "Class specifying that the only evaluation criterion is the lowest price"@en ;
  rdfs:isDefinedBy pc: .

# Contract award criterion
pc:AwardCriterion a owl:Class ;
  rdfs:subClassOf c4n:Condition ;
  rdfs:label "Kritérium pro udělení zakázky"@cs, "Contract award criterion"@en ;
  rdfs:comment "Třída pro popis jednoho kritéria a jeho váhy."@cs ;
  rdfs:comment "Class for description of one criterion and its weight"@en ;
  rdfs:isDefinedBy pc:
  .
  
#######
####### Terminologies
#######
  
####### Concept scheme of contract procedure types

pc:ProcedureTypeScheme a skos:ConceptScheme ; 
  skos:hasTopConcept pc:Open, pc:Restricted,
    pc:Negotiated, pc:CompetitiveDialogue ;
  rdfs:comment "Schéma konceptů pro druh řízení veřejné zakázky"@cs ;
  rdfs:comment "Concept scheme for description of procedure types for public contracts"@en
  .

pc:Open a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Otevřené"@cs, "Open"@en ;
  rdfs:comment "Otevřené řízení veřejné zakázky"@cs ;
  rdfs:comment "Open procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .

pc:Restricted a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Užší"@cs, "Restricted"@en ;
  rdfs:comment "Užší řízení veřejné zakázky"@cs ;
  rdfs:comment "Restricted procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:narrower pc:AcceleratedRestricted ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .
  
pc:AcceleratedRestricted a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Urychlené užší"@cs, "Accelerated restricted"@en ;
  rdfs:comment "Urychlené užší řízení veřejné zakázky"@cs ;
  rdfs:comment "Accelerated restricted procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:broader pc:Restricted
  .

pc:Negotiated a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Jednací"@cs, "Negotiated"@en ;
  rdfs:comment "Jednací řízení veřejné zakázky"@cs ;
  rdfs:comment "Negotiated procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:narrower pc:AcceleratedNegotiated ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .

pc:AcceleratedNegotiated a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Urychlené jednací"@cs, "Accelerated negotiated"@en ;
  rdfs:comment "Urychlené jednací řízení veřejné zakázky"@cs ;
  rdfs:comment "Accelerated negotiated procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:broader pc:Negotiated
  .

pc:CompetitiveDialogue a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Soutěžní dialog"@cs, "Competition dialogue"@en ;
  rdfs:comment "Soutěžní dialog jako typ řízení veřejné zakázky"@cs ;
  rdfs:comment "Competitive dialogue is the procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .
  
####### Concept scheme of kinds of public contracts

pc:KindScheme a skos:ConceptScheme ;
  skos:hasTopConcept pc:Supplies, pc:Works, pc:Services ;
  rdfs:comment "Schéma konceptů pro druh veřejné zakázky"@cs ;
  rdfs:comment "Concept scheme for kinds of public contracts"@en
  .

pc:Supplies a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Dodávky"@cs, "Supplies"@en ;
  rdfs:comment "Druhem veřejné zakázky jsou dodávky"@cs ;
  rdfs:comment "Supplies is the kind of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:KindScheme
  .

pc:Works a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Stavební práce"@cs, "Works"@en ;
  rdfs:comment "Druhem veřejné zakázky jsou stavební práce"@cs ;
  rdfs:comment "Works is the kind of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:KindScheme
  .

pc:Services a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Služby"@cs, "Services"@en ;
  rdfs:comment "Druhem veřejné zakázky jsou služby"@cs ;
  rdfs:comment "Services is the kind of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:KindScheme
  .

########
######## Properties
########

####### pc:Contract properties

# Contract main contact
pc:contact a owl:FunctionalProperty ;
  rdfs:label "Hlavní kontakt pro zakázku"@cs, "Main contact for a contract"@en ;
  rdfs:comment "Hlavní kontakt pro zakázku. Kardinalita 0..1"@cs ;
  rdfs:comment "Main contact for a contract. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range vcard:VCard ;
  rdfs:isDefinedBy pc:
  .

# Kind
pc:kind a owl:FunctionalProperty, qb:CodedProperty ;
  rdfs:label "Druh zakázky"@cs, "Type of contract"@en ;
  rdfs:comment "Druh zakázky. Kardinalita 0..1"@cs ;
  rdfs:comment "Type or kind of contract. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range skos:Concept, [
    a owl:Restriction ;
    owl:onProperty skos:inScheme ;
    owl:hasValue pc:KindScheme
  ] ;
  rdfs:isDefinedBy pc:
  .
  
# Type of procedure
pc:procedureType a owl:FunctionalProperty, qb:CodedProperty ;
  rdfs:label "Druh řízení zakázky"@cs, "Type of procedure of contract"@en ;
  rdfs:comment "Druh řízení zakázky. Kardinalita 0..1"@cs ;
  rdfs:comment "Type of procedure of contract. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range skos:Concept, [
    a owl:Restriction ;
    owl:onProperty skos:inScheme ;
    owl:hasValue pc:ProcedureTypeScheme
  ] ;
  rdfs:isDefinedBy pc: ;
  owl:equivalentProperty loted:PR
  .

# Contracting authority
pc:contractingAuthority a owl:FunctionalProperty ;
  rdfs:subPropertyOf [
    owl:inverseOf c4n:issues
  ] ;
  rdfs:label "Zadavatel"@cs, "Contracting authority"@en ;
  rdfs:comment "Zadavatel zakázky. Kardinalita 0..1"@cs ;
  rdfs:comment "Contracting authority. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range gr:BusinessEntity ;
  rdfs:isDefinedBy pc:
  .

# Product or service requested in the contract
pc:item a rdf:Property ;
  rdfs:subPropertyOf dcterms:hasPart ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf ( pc:Contract pc:Tender )
  ] ;
  rdfs:range gr:Offering ;
  rdfs:comment "Vlastnost pro přiřazení položky veřejné zakázce. Kardinalita 0..*"@cs ;
  rdfs:comment "Property for attachment of an Item to a public contract. Cardinality 0..*"@en ;
  rdfs:isDefinedBy pc:
  .
  
# Main object 
pc:mainObject a owl:FunctionalProperty ;
  rdfs:subPropertyOf dcterms:subject ;
  rdfs:label "Hlavní předmět zakázky"@cs, "Main object of contract"@en ;
  rdfs:comment "Vlastnost pro hlavní předmět veřejné zakázky. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for main object of public contract. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy pc:
  .
  
# Additional object 
pc:additionalObject a rdf:Property ;
  rdfs:subPropertyOf dcterms:subject ;
  rdfs:label "Další předmět zakázky"@cs, "Additional object of contract"@en ;
  rdfs:comment "Vlastnost pro další předmět veřejné zakázky. Kardinalita 0..*"@cs ;
  rdfs:comment "Property for additional object of public contract. Cardinality 0..*"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy pc:
  .

# Location
pc:location a rdf:Property ;
  rdfs:label "Místo uskutečnění zakázky"@cs, "Place of contract's realization"@en ;
  rdfs:comment "Místo, kde dojde k dodání produktu nebo služby nasmlouvané ve veřejné zakázce. Kardinalita 0..*"@cs ;
  rdfs:comment "Place of delivery of contracted product or service. Cardinality 0..*"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range schema:Place ;
  rdfs:isDefinedBy pc:
  .

####### ABSTRACT
# Price
pc:contractPrice a rdf:Property ;
  rdfs:label "Cena"@cs, "Price"@en ;
  rdfs:comment "Abstraktní vlastnost pro cenu veřejné zakázky"@cs ;
  rdfs:comment "Abstract property for price of public contract"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range gr:PriceSpecification ;
  rdfs:isDefinedBy pc:
  .
  
# Exact estimated contract price
pc:estimatedPrice a owl:FunctionalProperty ;
  rdfs:subPropertyOf pc:contractPrice ;
  rdfs:label "Přesná odhadovaná cena cena"@cs, "Exact estimated price"@en ;
  rdfs:comment "Vlastnost pro přesnou odhadovanou cenu veřejné zakázky. Pokud je odhadovanou cenou interval, použijte vlastnosti pc:estimatedPriceLower a pc:estimatedPriceUpper. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for exact estimated price of public contract. If only estimated interval is known, use pc:estimatedPriceLower and pc:estimatedPriceUpper properties instead. Cardinality 0..1"@en ;
  rdfs:isDefinedBy pc:
  .
  
# Lower range of estimated contract price
pc:estimatedPriceLower a owl:FunctionalProperty ;
  rdfs:subPropertyOf pc:contractPrice ;
  rdfs:label "Dolní mez odhadované ceny"@cs, "Lower range of estimated price"@en ;
  rdfs:comment "Vlastnost pro dolní mez odhadované ceny veřejné zakázky. Pokud je známa přesná odhadovaná cena, použijte vlastnost pc:estimatedPrice. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for lower bound of estimated price of public contract. If exact estimated price is known, use pc:estimatedPrice instead. Cardinality 0..1"@en ;
  rdfs:isDefinedBy pc:
  .
  
# Upper range of estimated contract price
pc:estimatedPriceUpper a owl:FunctionalProperty ;
  rdfs:subPropertyOf pc:contractPrice ;
  rdfs:label "Horní mez odhadované ceny"@cs, "Upper range of estimated price"@en ;
  rdfs:comment "Vlastnost pro horní mez odhadované ceny veřejné zakázky. Pokud je známa přesná odhadovaná cena, použijte vlastnost pc:estimatedPrice. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for upper bound of estimated price of public contract. If exact estimated price is known, use pc:estimatedPrice instead. Cardinality 0..1"@en ;
  rdfs:isDefinedBy pc:
  .

# Exact duration of contract in days
pc:durationDays a owl:FunctionalProperty ;
  rdfs:label "Doba trvání zakázky ve dnech"@cs, "Exact duration of contract in days"@en ;
  rdfs:comment "Vlastnost pro dobu trvání zakázky ve dnech. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for exact duration of contract in days. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:positiveInteger ;
  rdfs:isDefinedBy pc:
  .

# Time-limit for receipt of tenders or requests to participate
pc:tenderDeadline a owl:FunctionalProperty ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:label "Lhůta pro doručení nabídek nebo přijetí žádostí o účast"@cs,
    "Time-limit for receipt of tenders or requests to participate"@en ;
  rdfs:comment "Vlastnost pro lhůtu pro doručení nabídek nebo přijetí žádostí o účast. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for time limit for receipt of tenders or requests to participate. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:isDefinedBy pc: ;
  owl:equivalentProperty loted:DT
  .
  
# Contract publication date
pc:publicationDate a owl:FunctionalProperty ;
  rdfs:label "Datum zveřejnění zakázky"@cs, "Contract publication date"@en ;
  rdfs:comment "Vlastnost pro lhůtu pro doručení nabídek nebo přijetí žádostí o účast. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for time limit for receipt of tenders or requests to participate. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:issued ;
  rdfs:isDefinedBy pc: ;
  owl:equivalentProperty loted:PD
  .
  
# Date of contract award
pc:awardDate a owl:FunctionalProperty ;
  rdfs:label "Datum přidělení zakázky"@cs, "Date of contract award"@en ;
  rdfs:comment "Vlastnost pro datum přidělení zakázky dodavateli. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for date of contract award to a supplier. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc:
  .
  
# Start date of contract
pc:startDate a owl:FunctionalProperty ;
  rdfs:label "Datum začátku realizace zakázky"@cs, "Start date of contract"@en ;
  rdfs:comment "Vlastnost pro datum začátku realizace zakázky, které je uvedené ve smlouvě. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for start date of public contract realization coming from the contract's text. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc:
  .
  
# End date of contract
pc:estimatedEndDate a owl:FunctionalProperty ;
  rdfs:label "Očekávané datum konce realizace zakázky"@cs, "Estimated end date of contract"@en ;
  rdfs:comment "Vlastnost pro očekávané datum konce realizace zakázky. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for estimated end date of public contract realization. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc:
  .

# Actual end date of contact's realization
pc:actualEndDate a owl:FunctionalProperty ; 
  rdfs:label "Skutečné datum konce realizace zakázky"@cs, "Actual end date of contract"@en ;
  rdfs:comment "Vlastnost pro skutečné datum konce realizace zakázky. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for actual end date of public contract's realization. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc:
  .

# Date of cancellation
pc:cancellationDate a owl:FunctionalProperty ;
  rdfs:label "Datum zrušení zakázky"@cs, "Date when the contract was cancelled"@en ;
  rdfs:comment "Vlastnost určující datum, kdy byla zakázka zrušena. Kardinalita 0..1"@cs ;
  rdfs:comment  "Property specifying the date when the contract was cancelled. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc:
  .

# Combination of contract award criteria
pc:awardCriteriaCombination a owl:FunctionalProperty ;
  rdfs:label "Kombinace kritérií pro hodnocení nabídek dodavatelů zakázky."@cs, "Criteria combination for tender evaluation."@en ;
  rdfs:comment "Vlastnost pro nastavení kombinace kritérií pro hodnocení nabídek dodavatelů. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for criteria combination for tender evaluation. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range pc:AwardCriteriaCombination ;
  rdfs:isDefinedBy pc:
  .
  
# Contract award criterion
pc:awardCriterion a rdf:Property ;
  rdfs:label "Kritérium pro zadání zakázky."@cs, "Critera for contract award."@en, 
    "Critères d'attribution"@fr, "Vergabekriterien"@de ;
  rdfs:comment "Vlastnost pro nastavení jednoho kritéria pro hodnocení nabídek dodavatelů. Kardinalita 0..*"@cs ;
  rdfs:comment "Property for setting a criterion for tender evaluation. Cardinality 0..*"@en ;
  rdfs:domain pc:AwardCriteriaCombination ;
  rdfs:range pc:AwardCriterion ;
  rdfs:isDefinedBy pc: ;
  owl:equivalentProperty loted:AC
  .

# Contract file reference number
pc:referenceNumber a owl:FunctionalProperty ;
  rdfs:label "Spisové číslo zakázky"@cs, "File reference number"@en ;
  rdfs:comment "Vlastnost pro spisové číslo zakázky. Kardinalita 0..1"@cs ;
  rdfs:comment "Property for a public contract file reference number. Cardinality 0..1"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:string ;
  rdfs:isDefinedBy pc:
  .

# Contract lot
pc:lot a rdf:Property ;  
  rdfs:domain pc:Contract ;
  rdfs:range pc:Contract ;
  rdfs:label "Část zakázky"@cs, "Contract lot"@en ;
  rdfs:comment "Vlastnost pro specifikaci části zakázky. Zakázku rozděluje na části zadavatel, pokud je zakázka přiliš velká. Dodavatelé podávají nabídky na každou část odděleně a pro každou část zakázky je vybrán samostatný dodavatel. Pro samotnou zakázku nabídky nejsou podávány. Kardinalita 0..*"@cs ;
