  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc: .
  
# End date of contract
# 0..1
pc:estimatedEndDate a owl:FunctionalProperty ;
  rdfs:label "Očekávané datum konce realizace zakázky"@cs, "Estimated end date of contract"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc: .

# 0..1
pc:actualEndDate a owl:FunctionalProperty ; 
  rdfs:label "Skutečné datum konce realizace zakázky"@cs, "Actual end data of contract"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range xsd:date ;
  rdfs:subPropertyOf dcterms:date ;
  rdfs:isDefinedBy pc: .

# Type of contract award criteria
# 0..1
pc:awardCriteriaType a owl:FunctionalProperty ;
  rdfs:label "Typ kritéria pro zadání zakázky."@cs, "Type of critera for contract award."@en ;
  rdfs:domain pc:Contract ;
  rdfs:range pc:AwardCriteriaTypeCombination ;
  rdfs:isDefinedBy pc: .
  
# Contract award criteria
# 0..1
pc:awardCriteria a rdf:Property ;
  rdfs:label "Kritérium pro zadání zakázky."@cs, "Critera for contract award."@en, 
    "Critères d'attribution"@fr, "Vergabekriterien"@de ;
  rdfs:domain pc:AwardCriteriaTypeCombination ;
  rdfs:range pc:AwardCriteria ;
  rdfs:isDefinedBy pc: ;
