  rdfs:range xsd:nonNegativeInteger ;
  rdfs:subPropertyOf dcterms:issued ;
  rdfs:isDefinedBy pc: .
  
# Tender
# 0..*
pc:tender a rdf:Property ;
  rdfs:label "Nabídka"@cs, "Tender"@en ;
  rdfs:domain pc:Contract ;
  rdfs:range pc:Tender ;
  rdfs:isDefinedBy pc: .
  
# Awarded tender
# 0..1
