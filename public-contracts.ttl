####### 
####### Classes
#######

# Public contract
pc:Contract a owl:Class ;
  rdfs:subClassOf c4n:Call ;
  rdfs:label "Veřejná zakázka"@cs, "Public contract"@en ;
  rdfs:comment "Základní třída pro popis veřejné zakázky"@cs ;
  rdfs:comment "Basic class for decribing a public contract"@en ;
  rdfs:isDefinedBy pc: ;
  owl:equivalentClass loted:Tender
  .

# Framework agreement
pc:FrameworkAgreement a owl:Class ;
  rdfs:subClassOf pc:Contract ;
  rdfs:label "Rámcová smlouva"@cs, "Framework agreement"@en ;
  rdfs:comment "Třída pro popis rámcové smlouvy"@cs ;
  rdfs:comment "Class for decribing a framework agreement"@en ;
  rdfs:isDefinedBy pc: 
  .

# Public notice
pc:Notice a owl:Class ;
  rdfs:subClassOf irw:InformationResource ;
  rdfs:label "Oznámení"@cs, "Notice"@en ;
  rdfs:isDefinedBy pc: 
  .

# Prior information notice
pc:PriorInformationNotice a owl:Class ;
  rdfs:subClassOf pc:Notice ;
  rdfs:label "Předběžné oznámení o zakázce"@cs, "Prior information notice"@en ;
  rdfs:isDefinedBy pc:
  .

# Contract notice
pc:ContractNotice a owl:Class ;
  rdfs:subClassOf pc:Notice ;
  rdfs:label "Oznámení o zakázce"@cs , "Contract notice"@en ;
  rdfs:isDefinedBy pc:
  .

# Cancellation notice
pc:CancellationNotice a owl:Class ;
  rdfs:subClassOf pc:Notice ;
  rdfs:label "Oznámení o zrušení celé veřejné zakázky"@cs , "Notice about cancellation of a whole public contract"@en ;
  rdfs:isDefinedBy pc:
  .
  
# Contract notice
pc:ContractAwardNotice a owl:Class ;
  rdfs:subClassOf pc:Notice ;
  rdfs:label "Oznámení o zadání zakázky"@cs , "Contract award notice"@en ;
  rdfs:isDefinedBy pc:
  .

# Contract amendment notice
pc:ContractAmendmentNotice a owl:Class ;
  rdfs:subClassOf pc:Notice ;
  rdfs:label "Oznámení o změně zakázky"@cs , "Contract amendment notice"@en ;
  rdfs:isDefinedBy pc:
  .

# Tender
pc:Tender a owl:Class ;
  rdfs:subClassOf gr:Offering ;
  rdfs:label "Nabídka"@cs, "Tender"@en ;
  rdfs:comment "Základní třída pro popis nabídky dodavatele"@cs ;
  rdfs:comment "Basic class for decribing a tender submitted by a supplier"@en ;
  rdfs:isDefinedBy pc:
  .
  
# Default contract award criteria type: combination of criteria (= most economically advantageous award criteria type)
pc:AwardCriteriaCombination a owl:Class ;
  owl:equivalentClass loted:AwardCriteria ;
  rdfs:label "Kombinace kritérií pro hodnocení dodaných nabídek"@cs, "Combination of contract award criteria"@en ;
  rdfs:comment "Třída pro popis kombinace kritérií pro posuzování nabídek dodavatelů"@cs ;
  rdfs:comment "Class for description of criteria combination used for tender evaluation"@en ;
  rdfs:isDefinedBy pc:
  .
 
# Contract award criteria type: lowest price
pc:AwardCriteriaLowestPrice a owl:Class ;
  rdfs:subClassOf pc:AwardCriteriaCombination ;
  rdfs:subClassOf [
	a owl:Restriction ;
    owl:onProperty pc:awardCriterion ;
    owl:maxCardinality "0"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Nejnižší cena je jediným rozhodujícím kritériem při hodnocení dodaných nabídek"@cs, "Lowest price is the only contract award criterion"@en ;
  rdfs:comment "Třída specifikující že jediným kritériem pro posuzování nabídek dodavatelů je nejnižší cena"@cs ;
  rdfs:comment "Class specifying that the only evaluation criterion is the lowest price"@en ;
  rdfs:isDefinedBy pc: .

# Contract award criterion
pc:AwardCriterion a owl:Class ;
  rdfs:subClassOf c4n:Condition ;
  rdfs:label "Kritérium pro udělení zakázky"@cs, "Contract award criterion"@en ;
  rdfs:comment "Třída pro popis jednoho kritéria a jeho váhy."@cs ;
  rdfs:comment "Class for description of one criterion and its weight"@en ;
  rdfs:isDefinedBy pc:
  .
  
#######
####### Terminologies
#######
  
####### Concept scheme of types of contracting authorities

pc:AuthorityTypeScheme a skos:ConceptScheme ;
  skos:hasTopConcept pc:NationalAuthority, pc:NationalAgency, pc:LocalAuthority, pc:LocalAgency, pc:PublicBody, pc:InternationalOrganization ;
  rdfs:comment "Schéma konceptů pro typy veřejných zadavatelů"@cs ;
  rdfs:comment "Concept scheme for kinds of constracting authorities"@en
  .

pc:NationalAuthority a skos:Concept ;
  skos:inScheme pc:AuthorityTypeScheme ;
  rdfs:label "Ministerstvo nebo jiný celostátní či federální orgán"@cs, "Ministry or any other national or federal authority"@en ;
  rdfs:comment "Ministerstvo nebo jiný celostátní či federální orgán včetně jejich organizačních složek"@cs ;
  rdfs:comment "Ministry or any other national or federal authority, including their regional or local sub-divisions"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:AuthorityTypeScheme
  .

pc:NationalAgency a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Celostátní či federální úřad/agentura"@cs, "National or federal agency/office"@en ;
  rdfs:comment "Celostátní či federální úřad/agentura"@cs ;
  rdfs:comment "National or federal agency/office"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:AuthorityTypeScheme
  .

pc:LocalAuthority a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Regionální či místní orgán"@cs, "Regional or local authority"@en ;
  rdfs:comment "Regionální či místní orgán"@cs ;
  rdfs:comment "Regional or local authority"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:AuthorityTypeScheme
  .

pc:LocalAgency a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Regionální či místní úřad/agentura"@cs, "Regional or local agency/office"@en ;
  rdfs:comment "Regionální či místní úřad/agentura"@cs ;
  rdfs:comment "Regional or local agency/office"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:AuthorityTypeScheme
  .

pc:PublicBody a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Veřejnoprávní instituce"@cs, "Body governed by public law"@en ;
  rdfs:comment "Veřejnoprávní instituce"@cs ;
  rdfs:comment "Body governed by public law"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:AuthorityTypeScheme
  .

pc:InternationalOrganization a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Evropská instituce/agentura nebo mezinárodní organizace"@cs, "European institution/agency or international organisation""@en ;
  rdfs:comment "Evropská instituce/agentura nebo mezinárodní organizace"@cs ;
  rdfs:comment "European institution/agency or international organisation"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:AuthorityTypeScheme
  .

