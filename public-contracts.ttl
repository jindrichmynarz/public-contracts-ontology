####### 
####### Classes
#######

# Public contract
pc:Contract a rdfs:Class ;
  rdfs:subClassOf c4n:Call ;
  rdfs:label "Veřejná zakázka"@cs, "Public contract"@en ;
  rdfs:comment "Základní třída pro popis veřejné zakázky"@cs ;
  rdfs:comment "Basic class for decribing a public contract"@en ;
  rdfs:isDefinedBy pc: ;
  owl:sameAs loted:Contract 
  .
  
# Tender
pc:Tender a rdfs:Class ;
  rdfs:subClassOf gr:Offering ;
  rdfs:label "Nabídka"@cs, "Tender"@en ;
  rdfs:comment "Základní třída pro popis nabídky dodavatele"@cs ;
  rdfs:comment "Basic class for decribing a tender submitted by a supplier"@en ;
  rdfs:isDefinedBy pc: ;
  owl:sameAs loted:Tender
  .
  
# Default contract award criteria type: combination of criteria (= most economically advantageous award criteria type)
pc:AwardCriteriaTypeCombination a rdfs:Class ;
  rdfs:label "Kombinace kritérií pro hodnocení dodaných nabídek"@cs, "Combination of contract award criteria"@en ;
  rdfs:comment "Třída pro popis kombinace kritérií pro posuzování nabídek dodavatelů"@cs ;
  rdfs:comment "Class for description of criteria combination used for tender evaluation"@en ;
  rdfs:isDefinedBy pc: .
 
# Contract award criteria type: lowest price
pc:AwardCriteriaTypeLowestPrice a owl:Class ;
  rdfs:subClassOf pc:AwardCriteriaTypeCombination ;
  rdfs:subClassOf [
	a owl:Restriction ;
    owl:onProperty pc:awardCriteria ;
    owl:maxCardinality "0"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Nejnižší cena je jediným rozhodujícím kritériem při hodnocení dodaných nabídek"@cs, "Lowest price is the only contract award criterium"@en ;
  rdfs:comment "Třída specifikující že jediným kritériem pro posuzování nabídek dodavatelů je nejnižší cena"@cs ;
  rdfs:comment "Class specifying that the only evaluation crierion is the lowest price"@en ;
  rdfs:isDefinedBy pc: .

# Contract award criteria
pc:AwardCriteria a rdfs:Class ;
  rdfs:subClassOf c4n:Condition ;
  rdfs:label "Kritérium pro udělení zakázky"@cs, "Contract award criteria"@en ;
  rdfs:comment "Třída pro popis jednoho kritéria a jeho váhy."@cs ;
  rdfs:comment "Class for description of one criterion and its weight"@en ;
  rdfs:isDefinedBy pc: ;
  owl:sameAs loted:AwardCriteria
  .
  
#######
####### Terminologies
#######
  
####### Concept scheme of contract procedure types

pc:ProcedureTypeScheme a skos:ConceptScheme ; 
  skos:hasTopConcept pc:Open, pc:Restricted,
    pc:Negotiated, pc:CompetitiveDialogue ;
  rdfs:comment "Schéma konceptů pro druh řízení veřejné zakázky"@cs ;
  rdfs:comment "Concept scheme for description of procedure types for public contracts"@en
  .

pc:Open a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Otevřené"@cs, "Open"@en ;
  rdfs:comment "Otevřené řízení veřejné zakázky"@cs ;
  rdfs:comment "Open procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .

pc:Restricted a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Užší"@cs, "Restricted"@en ;
  rdfs:comment "Užší řízení veřejné zakázky"@cs ;
  rdfs:comment "Restricted procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:narrower pc:AcceleratedRestricted ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .
  
pc:AcceleratedRestricted a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Urychlené užší"@cs, "Accelerated restricted"@en ;
  rdfs:comment "Urychlené užší řízení veřejné zakázky"@cs ;
  rdfs:comment "Accelerated restricted procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:broader pc:Restricted
  .

pc:Negotiated a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Jednací"@cs, "Negotiated"@en ;
  rdfs:comment "Jednací řízení veřejné zakázky"@cs ;
  rdfs:comment "Negotiated procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:narrower pc:AcceleratedNegotiated ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .

pc:AcceleratedNegotiated a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Urychlené jednací"@cs, "Accelerated negotiated"@en ;
  rdfs:comment "Urychlené jednací řízení veřejné zakázky"@cs ;
  rdfs:comment "Accelerated negotiated procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:broader pc:Negotiated
  .

pc:CompetitiveDialogue a skos:Concept ;
  skos:inScheme pc:ProcedureTypeScheme ;
  rdfs:label "Soutěžní dialog"@cs, "Competition dialogue"@en ;
  rdfs:comment "Soutěžní dialog jako typ řízení veřejné zakázky"@cs ;
  rdfs:comment "Competitive dialogue is the procedure type of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:ProcedureTypeScheme
  .
  
####### Concept scheme of kinds of public contracts

pc:KindScheme a skos:ConceptScheme ;
  skos:hasTopConcept pc:Supplies, pc:Works, pc:Services ;
  rdfs:comment "Schéma konceptů pro druh veřejné zakázky"@cs ;
  rdfs:comment "Concept scheme for kinds of public contracts"@en
  .

pc:Supplies a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Dodávky"@cs, "Supplies"@en ;
  rdfs:comment "Druhem veřejné zakázky jsou dodávky"@cs ;
  rdfs:comment "Supplies is the kind of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:KindScheme
  .

pc:Works a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Stavební práce"@cs, "Works"@en ;
  rdfs:comment "Druhem veřejné zakázky jsou stavební práce"@cs ;
  rdfs:comment "Works is the kind of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:KindScheme
  .

pc:Services a skos:Concept ;
  skos:inScheme pc:KindScheme ;
  rdfs:label "Služby"@cs, "Services"@en ;
  rdfs:comment "Druhem veřejné zakázky jsou služby"@cs ;
  rdfs:comment "Services is the kind of public contract"@en ;
  rdfs:isDefinedBy pc: ;
  skos:topConceptOf pc:KindScheme
  .

